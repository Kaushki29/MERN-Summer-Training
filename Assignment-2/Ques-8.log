function phone(num)
    {
        var c=0;
        var y=num;
        var d;
        while(y>0)
            {
                d=y%10;
                y=parseInt(y/10);
                if(d>=0 && d<=9)
                {
                    c++;
                    continue;
                }
                else
                break;
            }
        if(c!=10)
            console.log("Invalid phone no.");
        else
            console.log("Valid phone no.");
    }
