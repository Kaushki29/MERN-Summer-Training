function Occurence(str)
    {
        var map=new Map();
        for(var i=0;i<str.length;i++)
            {
                if(map.has(str.charAt(i)))
                {
                    var c=map.get(str.charAt(i));
                    map.set(str.charAt(i),c+1);
                }
                else
                    map.set(str.charAt(i),1);
            }
        for(var[key,value] of map)
            console.log(`${key} = ${value}`);
    }

