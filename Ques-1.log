date=new Date();
var days=["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];
var day=days[date.getDay()];
var h=date.getHours();
var m=date.getMinutes();
var s=date.getSeconds();
var set="am";
if(h>12)
{
    h=h-12;
    set="pm";
}
console.log("Today is: "+day);
console.log(`Current Time is: ${h}:${m}:${s}`+set);
